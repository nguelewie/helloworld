  agent any 
  stages {
    stage('build'){
      steps {
        echo "build steps" 
        sleep 5
      }
    }
    stage('test'){
      steps {
        echo "test steps" 
        sleep 5
      }
    }
    stage('deploy'){
      steps {
        echo "deploy steps" 
        sleep 5
      }
    }
    stage('docker'){
      steps {
        echo "image" 
        sleep 5
      }
    }
  }
}
